<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="home1RowModel" type="com.example.app.modules.home.data.model.Home1RowModel"/>
    </data>
    <FrameLayout android:layout_height="wrap_content" android:layout_width="wrap_content">
        <ImageView android:layout_gravity="center"
            android:layout_height="@dimen/_117px" android:layout_width="@dimen/_156px"/>
        <LinearLayout android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:fontFamily="@font/avenir"
                android:gravity="top|start"
                android:id="@+id/txt21102020At0"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{home1RowModel.lbl21102020At0}"
                android:textColor="@color/gray_900"
                android:textFontWeight="400"
                android:textSize="@dimen/_12px" android:textStyle="bold"/>
            <View android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1px"
                android:layout_marginEnd="@dimen/_1px"
                android:layout_marginTop="@dimen/_10px" android:layout_width="match_parent"/>
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24px"
                android:layout_marginTop="@dimen/_10px"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start"
                    android:id="@+id/txtShape"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.32"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lblShape}"
                    android:textColor="@color/black_100"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start"
                    android:id="@+id/txtRoundPear"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6px"
                    android:layout_weight="0.68"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lblRoundPear}"
                    android:textColor="@color/gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29px"
                android:layout_marginTop="@dimen/_6px"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start"
                    android:id="@+id/txtCarat"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.29"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lblCarat}"
                    android:textColor="@color/black_100"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start"
                    android:id="@+id/txt7051205"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11px"
                    android:layout_weight="0.71"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lbl7051205}"
                    android:textColor="@color/gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6px"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start"
                    android:id="@+id/txtColor"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.39"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lblColor}"
                    android:textColor="@color/black_100"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
                <TextView android:fontFamily="@font/avenir"
                    android:gravity="top|start" android:id="@+id/txtDEF"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10px"
                    android:layout_weight="0.61"
                    android:layout_width="0dp"
                    android:text="@{home1RowModel.lblDEF}"
                    android:textColor="@color/gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12px" android:textStyle="bold"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
